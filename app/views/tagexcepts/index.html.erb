<p id="notice"><%= notice %></p>

<h1>Listing Tagexcepts</h1>

<table>
  <thead>
    <tr>
      <th>Tag</th>
      <th>Name</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @tagexcepts.each do |tagexcept| %>
      <tr>
        <td><%= tagexcept.tag_id %></td>
        <td><%= tagexcept.name %></td>
        <td><%= link_to 'Show', tagexcept %></td>
        <td><%= link_to 'Edit', edit_tagexcept_path(tagexcept) %></td>
        <td><%= link_to 'Destroy', tagexcept, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Tagexcept', new_tagexcept_path %>
